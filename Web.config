<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<connectionStrings>
		<add name="connectionToDB" connectionString="Data Source=(localdb)\mssqllocaldb;Initial Catalog=ChatAppDB;Integrated Security=True" providerName="System.Data.SqlClient" />
	</connectionStrings>


  <system.web>
    <customErrors mode="Off" />
  
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1" executionTimeout="240" maxRequestLength="2048000" />
 
  <pages>
      <controls>
        <add tagPrefix="ajaxToolkit" assembly="AjaxControlToolkit" namespace="AjaxControlToolkit" />
      </controls>
    </pages></system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <clear />
        <add value="Index.aspx" />
        <add value="Default.htm" />
        <add value="Default.asp" />
        <add value="index.htm" />
        <add value="index.html" />
        <add value="iisstart.htm" />
        <add value="default.aspx" />
      </files>
    </defaultDocument>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="1073741824"/>
      </requestFiltering>
    </security>
    <directoryBrowse enabled="true" />
  </system.webServer>


</configuration>